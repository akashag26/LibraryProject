<%= stylesheet_link_tag 'rooms' %>
<h1>Listing Rooms</h1>

<table align="center">
  <thead>
  <tr>
    <th>Room No</th>
    <th>10:00 - 12:00</th>
    <th>12:00 - 2:00</th>
    <th>2:00 - 4:00</th>
    <th>4:00 - 6:00</th>
    <th>6:00 - 8:00</th>
    <th>8:00 - 10:00</th>
  </tr>
  </thead>

  <tbody>
  <% @rooms.each do |room| %>
      <tr>
        <th scope="row"><%= room.room_no %></th>
        <%6.times do || %>
          <td>Available</td>
        <%end%>
      </tr>
  <% end %>
  </tbody>
</table>

<br>

