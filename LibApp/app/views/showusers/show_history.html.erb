<p id="notice"><%= notice %></p>

<h1>Users</h1>

<table>
  <thead>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @details.each do |detail| %>
      <tr>
        <td><%= detail.firstname %></td>
        <td><%= detail.email %></td>
        <td><%= link_to 'Show Reservation History', {:controller => "booking_histories", :action =>
            "showuserhistory", :email => detail.email} %></td>
      </tr>
  <% end %>
  </tbody>
</table>

<br>